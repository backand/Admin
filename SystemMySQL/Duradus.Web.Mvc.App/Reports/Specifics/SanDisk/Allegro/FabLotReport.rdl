    <DataSet Name="FabLotChart">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Technology">
            <Value>=Parameters!Technology.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Fab">
            <Value>=Parameters!Fab.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select Fab.Name AS FAB,Technology.Name AS Technology, Fablot.Date, cast(Fablot.Lot As Integer) AS Lot,Fablot.FabId, Technology.Ordinal, TechnologyId
from FabLot WITH (NOLOCK)
INNER JOIN Fab WITH (NOLOCK) ON fab.Id = Fablot.FabId 
INNER JOIN Technology WITH (NOLOCK) ON Technology.Id = FabLot.TechnologyId
WHERE  (FabLot.Date &gt;= @FromDate) AND (FabLot.Date &lt;= @ToDate) AND (FabLot.TechnologyId IN (@Technology)) AND (FabLot.FabId IN (@Fab))
UNION ALL
select Fab.Name AS FAB,'NA' AS Technology, t.Date,(MaxLots - lots) AS Lot,t.FabId, 1000 AS Ordinal, -1 AS TechnologyId
from
(select Date, FabId, cast(SUM(Lot) as int) as Lots
from FabLot WITH (NOLOCK) group by Date, FabId) as t
inner join 
(select FabId, MAX(Lots) AS MaxLots from
(select Date, FabId, cast(SUM(Lot) as int) as lots
from FabLot WITH (NOLOCK) group by Date, FabId) AS t1
group by FabId) as t2 ON t.FabId = t2.FabId
INNER JOIN Fab WITH (NOLOCK) ON fab.Id = t.FabId
WHERE  (t.Date &gt;= @FromDate AND t.Date &lt;= @ToDate) AND (t.FabId IN (@Fab))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FAB">
          <DataField>FAB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Technology">
          <DataField>Technology</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date">
